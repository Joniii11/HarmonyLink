{
  "name": "harmonylink",
  "version": "2.0.0",
  "description": "Seamlessly connect to Lavalink, NodeLink, and FrequenC nodes for high-quality audio streaming in your Discord bots. Supports Discord.js, Eris, Oceanic.js, and a powerful plugin system.",
  "main": "./dist/index.js",  "scripts": {
    "build": "tspc && npm run build:esm",
    "build:esm": "gen-esm-wrapper dist/index.js dist/index.mjs",
    "lint": "eslint src/**/*.ts",
    "docs": "typedoc",
    "docs:clean": "rimraf docs && npm run docs",
    "docs:watch": "typedoc --watch",
    "docs:serve": "typedoc --serve",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prepublishOnly": "npm run prepare && npm run build",
    "prepare": "ts-patch install -s"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Joniii11/HarmonyLink.git"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "keywords": [
    "LavaLink",
    "NodeLink",
    "FrequenC",
    "connect"
  ],
  "author": "Joniii11",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Joniii11/HarmonyLink/issues"
  },
  "prettier": {
    "tabWidth": 4,
    "trailingComma": "none",
    "arrowParens": "avoid",
    "printWidth": 120
  },
  "homepage": "https://github.com/Joniii11/HarmonyLink#readme",
  "dependencies": {
    "neverthrow": "^8.2.0",
    "ws": "^8.18.2"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.9"
  },  "devDependencies": {
    "@clytage/eslint-config": "^3.0.1",
    "@types/node": "^22.14.1",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.34.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-neverthrow": "^1.1.4",
    "eslint-plugin-prettier": "^5.1.3",
    "gen-esm-wrapper": "^1.1.3",
    "prettier": "^3.3.1",
    "ts-patch": "^3.3.0",
    "typedoc": "^0.26.4",
    "typedoc-plugin-markdown": "^4.2.1",
    "typescript-transform-paths": "^3.4.7"
  },
  "_moduleAliases": {
    "@": "./dist/",
    "@t": "./dist/typings"
  }
}
