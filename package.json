{
  "name": "harmonylink",
  "version": "1.2.4",
  "description": "HarmonyLink: Seamlessly connect to LavaLink nodes for high-quality audio streaming in your applications. HarmonyLink offers a reliable and easy-to-use interface, perfect for building music bots and audio applications with superior performance.",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tspc && npm run build:esm",
    "build:esm": "gen-esm-wrapper dist/index.js dist/index.mjs",
    "prepublishOnly": "npm run prepare && npm run build && npm run lint",
    "lint": "eslint . --cache --cache-file .eslintcache --ignore-path .gitignore --ext .ts",
    "lint:fix": "eslint . --cache --cache-file .eslintcache --ignore-path .gitignore --ext .ts --fix",
    "prepare": "ts-patch install -s"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Joniii11/HarmonyLink.git"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "keywords": [
    "LavaLink",
    "NodeLink",
    "FrequenC",
    "connect"
  ],
  "author": "Joniii11",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Joniii11/HarmonyLink/issues"
  },
  "eslintConfig": {
    "extends": [
      "@clytage/eslint-config/typescript",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "ignorePatterns": [
      "dist/*"
    ],
    "rules": {
      "@typescript-eslint/no-extra-parens": "off",
      "@typescript-eslint/no-unsafe-member-access": "off",
      "@typescript-eslint/no-unsafe-call": "off",
      "@typescript-eslint/no-type-alias": "off",
      "@typescript-eslint/no-empty-function": "off"
    }
  },
  "prettier": {
    "tabWidth": 4,
    "trailingComma": "none",
    "arrowParens": "avoid",
    "printWidth": 120
  },
  "homepage": "https://github.com/Joniii11/HarmonyLink#readme",
  "dependencies": {
    "ws": "^8.18.0"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.8"
  },
  "devDependencies": {
    "@clytage/eslint-config": "^3.0.1",
    "@types/node": "^20.12.12",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "gen-esm-wrapper": "^1.1.3",
    "prettier": "^3.3.1",
    "ts-patch": "^3.2.1",
    "typedoc": "^0.26.4",
    "typedoc-plugin-markdown": "^4.2.1",
    "typescript-transform-paths": "^3.4.7"
  },
  "_moduleAliases": {
    "@": "./dist/",
    "@t": "./dist/typings"
  }
}
